# Environment Variables - Whiskey Sensory Training App
# Copy this file to .env and fill in your actual values

# Flask Configuration
FLASK_ENV=production
FLASK_APP=app.py

# Database Path (Railway will use this)
DB_PATH=/app/databases/whiskey_production.db

# Frontend URL for CORS (update with your actual Vercel domain)
FRONTEND_URL=https://whiskey-training-app.vercel.app

# Port (Railway provides this automatically)
PORT=5000
